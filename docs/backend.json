{
  "entities": {
    "UserProfile": {
      "title": "User Profile",
      "description": "Represents a user's profile information.",
      "type": "object",
      "properties": {
        "id": { "type": "string", "format": "uuid" },
        "email": { "type": "string", "format": "email" },
        "firstName": { "type": "string" },
        "lastName": { "type": "string" },
        "role": { "type": "string", "enum": ["admin", "editor", "user"] },
        "phone": { "type": "string" },
        "company": { "type": "string" },
        "profileImage": { "type": "string", "format": "uri" },
        "bio": { "type": "string" },
        "isActive": { "type": "boolean" },
        "createdAt": { "type": "string", "format": "date-time" },
        "updatedAt": { "type": "string", "format": "date-time" }
      },
      "required": ["id", "email", "role"]
    },
    "Service": {
      "title": "Service",
      "description": "A regulatory service offered.",
      "type": "object",
      "properties": {
        "id": { "type": "string", "format": "uuid" },
        "title": { "type": "string" },
        "description": { "type": "string" },
        "shortDescription": { "type": "string" },
        "slug": { "type": "string" },
        "icon": { "type": "string" },
        "href": { "type": "string", "format": "uri" },
        "category": { "type": "string" },
        "content": { "type": "string" },
        "features": { "type": "array", "items": { "type": "object" } },
        "requirements": { "type": "array", "items": { "type": "object" } },
        "processingTime": { "type": "string" },
        "pricing": { "type": "object" },
        "image": { "type": "string", "format": "uri" },
        "thumbnail": { "type": "string", "format": "uri" },
        "published": { "type": "boolean" },
        "featured": { "type": "boolean" },
        "seoTitle": { "type": "string" },
        "seoDescription": { "type": "string" },
        "seoKeywords": { "type": "string" }
      },
      "required": ["id", "title", "description", "slug", "href", "category"]
    },
    "BlogPost": {
      "title": "Blog Post",
      "description": "A single blog post.",
      "type": "object",
      "properties": {
        "id": { "type": "string", "format": "uuid" },
        "title": { "type": "string" },
        "slug": { "type": "string" },
        "excerpt": { "type": "string" },
        "content": { "type": "string" },
        "category": { "type": "string" },
        "tags": { "type": "array", "items": { "type": "string" } },
        "readTime": { "type": "string" },
        "seoTitle": { "type": "string" },
        "seoDescription": { "type": "string" },
        "seoKeywords": { "type": "string" },
        "featuredImage": { "type": "string", "format": "uri" },
        "thumbnail": { "type": "string", "format": "uri" },
        "published": { "type": "boolean" },
        "publishedAt": { "type": "string", "format": "date-time" },
        "featured": { "type": "boolean" },
        "authorId": { "type": "string", "format": "uuid" },
        "authorName": { "type": "string" },
        "viewsCount": { "type": "integer" }
      },
      "required": ["id", "title", "slug", "content", "authorId"]
    },
    "Testimonial": {
      "title": "Testimonial",
      "description": "A customer testimonial.",
      "type": "object",
      "properties": {
        "id": { "type": "string", "format": "uuid" },
        "postUrl": { "type": "string", "format": "uri" },
        "createdAt": { "type": "string", "format": "date-time" }
      },
      "required": ["id", "postUrl"]
    },
    "FormSubmission": {
      "title": "Form Submission",
      "description": "A submission from a user via a form.",
      "type": "object",
      "properties": {
        "id": { "type": "string", "format": "uuid" },
        "formType": { "type": "string" },
        "status": { "type": "string", "enum": ["pending", "in-progress", "completed", "rejected", "archived"] },
        "serviceId": { "type": "string", "format": "uuid" },
        "serviceName": { "type": "string" },
        "fullName": { "type": "string" },
        "email": { "type": "string", "format": "email" },
        "phone": { "type": "string" },
        "company": { "type": "string" },
        "industry": { "type": "string" },
        "details": { "type": "object" },
        "internalNotes": { "type": "string" },
        "assignedTo": { "type": "string", "format": "uuid" },
        "createdAt": { "type": "string", "format": "date-time" },
        "updatedAt": { "type": "string", "format": "date-time" },
        "completedAt": { "type": "string", "format": "date-time" }
      },
      "required": ["id", "formType", "status", "fullName", "email"]
    }
  },
  "auth": {
    "providers": ["email-password"]
  }
}
